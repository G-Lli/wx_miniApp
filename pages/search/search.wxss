page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  width: 100%;
  display: flex;
}
.search_row input {
  flex: 8;
  background-color: #fff;
  height: 100%;
  padding-left: 10rpx;
}
.search_row button {
  flex: 2;
  width: 2rpx;
  height: 100%;
  font-size: 28rpx;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 10rpx;
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  background-color: #fff;
  padding: 10rpx 0;
  border-bottom: 1rpx solid #cccccc;
  font-size: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
